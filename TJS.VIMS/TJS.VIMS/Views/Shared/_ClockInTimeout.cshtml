@model TJS.VIMS.ViewModel.AuthorizedViewModel

@section scripts
{
    <script>
        setInterval(function () {
            var div = document.querySelector("#counter");
            var count = div.textContent * 1 - 1;
            div.textContent = count;
            if (count <= 0) {
                    location.href = "@Url.Action("VolunteerLookUp", "VolunteerClockTime", new { locationId = Model.UserId })";
            }
        }, 1000);
    </script>
}
